      <Trigger Name="UPDATE_ACTIVITY_DATE" FireInQuery="false" TriggerText="   :WORKDATE := :SYSTEM.CURRENT_BLOCK ;&amp;#10;--&amp;#10;   :WORKDATE := SUBSTR(:WORKDATE, 1, 7) ;&amp;#10;--&amp;#10;   :WORKDATE := RPAD(:WORKDATE, 21, '_ACTIVITY_DATE') ;&amp;#10;--&amp;#10;   COPY( :CURRENT_DATE, :WORKDATE ) ;" DisplayInKeyboardHelp="true"/>
      <Trigger Name="KEY-PRVBLK" FireInQuery="false" TriggerText="  G$_INVALID_FUNCTION_MSG;" DisplayInKeyboardHelp="false"/>
      <Trigger Name="KEY-NXTKEY" KeyboardHelpText="Exit With Value" FireInQuery="false" TriggerText="   :GLOBAL.VALUE := :GZVZIPC_BLOCK.GZVZIPC_CODE;&amp;#10;   :GLOBAL.VALUE_2 := :GZVZIPC_BLOCK.GZVZIPC_CITY;&amp;#10;   G$_GOQOLIB_KEY_TRIGGER.B2K_EXIT_FORM ;" DisplayInKeyboardHelp="true"/>
      <Trigger Name="KEY-NXTBLK" FireInQuery="false" TriggerText="  G$_INVALID_FUNCTION_MSG;" DisplayInKeyboardHelp="false"/>
      <Trigger Name="KEY-EXIT" KeyboardHelpText="Exit Without Value" FireInQuery="false" TriggerText="   :GLOBAL.VALUE := '' ;&amp;#10;   G$_GOQOLIB_KEY_TRIGGER.B2K_EXIT_FORM  ;" DisplayInKeyboardHelp="true"/>
      <Trigger Name="PRE-UPDATE" FireInQuery="false" TriggerText="   DECLARE&amp;#10;      PTI_INTO_TEMP        VARCHAR2(255);&amp;#10;      CURSOR PTI_CURSOR IS&amp;#10;         SELECT 'X'&amp;#10;         FROM   GZVZIPC&amp;#10;         WHERE  GZVZIPC_CODE = :GZVZIPC_BLOCK.GZVZIPC_CODE&amp;#10;         AND    UPPER(GZVZIPC_CITY) = UPPER(:GZVZIPC_BLOCK.GZVZIPC_CITY)&amp;#10;         AND    :GZVZIPC_BLOCK.ROWID &lt;> ROWID ;&amp;#10;      CURSOR PTI_CURSOR_2 IS&amp;#10;         SELECT 'X'&amp;#10;         FROM   GTVZIPC&amp;#10;         WHERE  GTVZIPC_CODE = :GZVZIPC_BLOCK.GZVZIPC_CODE&amp;#10;         AND    GTVZIPC_CITY = :GZVZIPC_BLOCK.GZVZIPC_CITY;&amp;#10;   BEGIN&amp;#10;      OPEN PTI_CURSOR ;&amp;#10;      FETCH PTI_CURSOR INTO PTI_INTO_TEMP ;&amp;#10;      G$_CHECK_VALUE (PTI_CURSOR%FOUND,&amp;#10;          G$_NLS.Get('GTVZIPC-0002', 'FORM','*ERROR* Invalid update; zip code already exists') , TRUE);&amp;#10;      OPEN PTI_CURSOR_2 ;&amp;#10;      FETCH PTI_CURSOR_2 INTO PTI_INTO_TEMP ;&amp;#10;      G$_CHECK_VALUE (PTI_CURSOR_2%NOTFOUND,&amp;#10;          '*ERROR* Invalid update; zip code must be defined on GTVZIPC first', TRUE);&amp;#10;   END ;&amp;#10;--&amp;#10;   EXECUTE_TRIGGER( 'UPDATE_ACTIVITY_DATE' ) ;&amp;#10;   G$_CHECK_FAILURE ;&amp;#10;   :GZVZIPC_USER_ID := :GLOBAL.CURRENT_USER ;" DisplayInKeyboardHelp="true"/>
      <Trigger Name="PRE-INSERT" FireInQuery="false" TriggerText="   DECLARE&amp;#10;      PTI_INTO_TEMP        VARCHAR2(255);&amp;#10;      CURSOR PTI_CURSOR IS&amp;#10;         SELECT 'X'&amp;#10;         FROM   GZVZIPC&amp;#10;         WHERE  GZVZIPC_CODE = :GZVZIPC_BLOCK.GZVZIPC_CODE&amp;#10;         AND    UPPER(GZVZIPC_CITY) = UPPER(:GZVZIPC_BLOCK.GZVZIPC_CITY);&amp;#10;      CURSOR PTI_CURSOR_2 IS&amp;#10;         SELECT 'X'&amp;#10;         FROM   GTVZIPC&amp;#10;         WHERE  GTVZIPC_CODE = :GZVZIPC_BLOCK.GZVZIPC_CODE&amp;#10;         AND    GTVZIPC_CITY = :GZVZIPC_BLOCK.GZVZIPC_CITY;&amp;#10;   BEGIN&amp;#10;      OPEN PTI_CURSOR ;&amp;#10;      FETCH PTI_CURSOR INTO PTI_INTO_TEMP ;&amp;#10;      G$_CHECK_VALUE (PTI_CURSOR%FOUND,&amp;#10;          G$_NLS.Get('GTVZIPC-0002', 'FORM','*ERROR* Invalid insert; zip code already exists') , TRUE);&amp;#10;      OPEN PTI_CURSOR_2 ;&amp;#10;      FETCH PTI_CURSOR_2 INTO PTI_INTO_TEMP ;&amp;#10;      G$_CHECK_VALUE (PTI_CURSOR_2%NOTFOUND,&amp;#10;          '*ERROR* Invalid insert; zip code must be defined on GTVZIPC first', TRUE);&amp;#10;   END ;&amp;#10;--&amp;#10;   EXECUTE_TRIGGER( 'UPDATE_ACTIVITY_DATE' ) ;&amp;#10;   G$_CHECK_FAILURE ;&amp;#10;   :GZVZIPC_USER_ID := :GLOBAL.CURRENT_USER ;" DisplayInKeyboardHelp="true"/>
